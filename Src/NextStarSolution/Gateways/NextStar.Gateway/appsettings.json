{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "SerilogSetting": {
    "ServerName": "IdentityService",
    "ErrorConfig": {
      "Path": "../Logs",
      "FileSizeLimitBytes": 5242880,
      "RetainedFileCountLimit": 300
    },
    "NormalConfig": {
      "path": "../Logs",
      "FileSizeLimitBytes": 5242880,
      "RetainedFileCountLimit": 200
    },
    "BusinessConfig": {
      "path": "../Logs",
      "FileSizeLimitBytes": 5242880,
      "RetainedFileCountLimit": 200
    }
  },
  "AllowedOrigins": [
    "https://localhost:12001"
  ],
  "Authority": "https://localhost:44385",
  "DataBaseSetting": {
    "Redis": "localhost"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6409
        }
      ],
      "UpstreamPathTemplate": "/api/kaikeimigration/{everything}",
      "UpstreamHttpMethod": [ "Put", "Delete", "Get", "Post" ],
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{RemoteIpAddress}"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "nextstarapi",
        "AllowedScopes": [ "nextstarapi" ]
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:44344"
  }
}
