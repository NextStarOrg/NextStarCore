{
  "ConfigEnvironment": "DEBUG",
  "IdentityServer": {
    "Clients": [
      {
        "Enabled": true,
        "ClientId": "nextstarmanagefront",
        "ClientName": "管理后台",
        "AllowedGrantTypes": [
          "authorization_code"
        ],
        "RequireClientSecret": false,
        "RedirectUris": [
          "http://localhost:12000/callback"
        ],
        "PostLogoutRedirectUris": [
          "http://localhost:12000/signoutcallback"
        ],
        "AllowedCorsOrigins": [
          "http://localhost:12000"
        ],
        "AllowedScopes": [
          "openid",
          "profile",
          "nextstarapi"
        ],
        "IdentityTokenLifetime": 604800,
        "AccessTokenLifetime": 604800
      }
    ]
  }
}