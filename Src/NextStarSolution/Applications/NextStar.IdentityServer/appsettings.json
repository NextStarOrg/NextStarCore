{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "SerilogSetting": {
    "ServerName": "IdentityService",
    "ErrorConfig": {
      "Path": "../Logs",
      "FileSizeLimitBytes": 5242880,
      "RetainedFileCountLimit": 300
    },
    "NormalConfig": {
      "path": "../Logs",
      "FileSizeLimitBytes": 5242880,
      "RetainedFileCountLimit": 200
    },
    "BusinessConfig": {
      "path": "../Logs",
      "FileSizeLimitBytes": 5242880,
      "RetainedFileCountLimit": 200
    }
  },
  "DataBaseSetting": {
    "Account": "Server=127.0.0.1;Database=NextStarAccount;User Id=sa;Password=xA123456;",
    "Redis": "localhost"
  },
  "ConfigKey": "PROD",
  "Certificates": {
    "Path": "IS4.pfx",
    "Password": "xA123456"
  },
  "IdentityServer": {
    "Clients": [
      {
        "Enabled": true,
        "ClientId": "nextstarmanagefront",
        "ClientName": "管理后台",
        "AllowedGrantTypes": [
          "authorization_code"
        ],
        "RequireClientSecret": false,
        "RedirectUris": [
          "https://localhost:12001/callback"
        ],
        "PostLogoutRedirectUris": [
          "https://localhost:12001/signoutcallback"
        ],
        "AllowedCorsOrigins": [
          "https://localhost:12001"
        ],
        "AllowedScopes": [
          "openid",
          "profile",
          "nextstarapi"
        ],
        "IdentityTokenLifetime": 604800,
        "AccessTokenLifetime": 259200
      }
    ]
  },
  "JavaScript": [
    {
      "Name": "Jquery",
      "Url": "https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"
    },
    {
      "Name": "Lodash",
      "Url": "https://cdn.jsdelivr.net/npm/lodash@4.17.21/lodash.min.js"
    },
    {
      "Name": "BootStrap",
      "Url": "https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"
    },
    {
      "Name": "FingerPrintJS",
      "Url": "https://cdn.jsdelivr.net/npm/@fingerprintjs/fingerprintjs@3.1.2/dist/fp.min.js"
    },
    {
      "Name": "Browser",
      "Url": "https://cdn.jsdelivr.net/npm/bowser@2.11.0/es5.min.js"
    },
    {
      "Name": "JqueryValidate",
      "Url": "https://cdn.jsdelivr.net/npm/jquery-validation@1.19.3/dist/jquery.validate.min.js"
    },
    {
      "Name": "JqueryValidateUnobtrusive",
      "Url": "https://cdn.jsdelivr.net/npm/jquery-validation-unobtrusive@3.2.12/dist/jquery.validate.unobtrusive.min.js"
    }
  ],
  "StyleSheet": [
    {
      "Name": "BootStrap",
      "Url": "https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css"
    }
  ]
}
